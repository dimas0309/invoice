<form class="d-flex mt-5">
    <select class="form-select form-select-sm me-2" 
            name="date" id="date">
            <% for (date of dates) {%>
                <option>
                    <%= (date.date).toLocaleDateString()%>
               </option>
              <%}%> 
          </select>
        <button class="btn btn-outline-success" type="submit">
            Search
        </button>
      </form>